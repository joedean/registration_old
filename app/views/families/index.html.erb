<div class="container">
  <div class="row">
    <h3>Families</h3>
    <table class="table">
      <tbody>
        <% @families.each do |family| %>
          <tr>
            <%= render family %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<div class="text-center">
  <%= will_paginate @families %>
</div>
<div class="container">
  <%= link_to("Create family", new_family_path, class: 'button-xs') %>
</div>
